# Server Configuration
PORT=3000  # Port for the backend server

# CORS Configuration
FRONTEND_ORIGIN=https://your-frontend-domain.com  # Replace with actual frontend URL

# AWS DynamoDB Configuration
AWS_REGION=your-aws-region  # e.g., ap-south-1
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key

# Admin Authentication
ADMIN_API_KEY=your-admin-api-key  # Used for protecting admin routes

# Socket.IO Configuration (if applicable)
SOCKET_TRANSPORTS=websocket,polling  # Optional

# Optional: Logging or Debugging
LOG_LEVEL=info  # Set to debug, info, warn, or error
